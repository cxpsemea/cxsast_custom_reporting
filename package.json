    "bugs": "https://github.com/cxpsemea/cxsast_custom_reporting/issues",
    "homepage": "https://github.com/cxpsemea/cxsast_custom_reporting#readme",
    "keywords": [
        "checkmarx",
        "sast",
        "report",
        "scan",
        "post-scan"
    ],
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "scripts": {
        "dev": "nodemon --watch source --exec ts-node source/main.ts --reportAudience user01@test.com,user02@test.com",
        "docs": "docsify serve ./docs"
    },
